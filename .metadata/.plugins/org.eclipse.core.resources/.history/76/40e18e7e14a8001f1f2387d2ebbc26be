package BAsice;

import java.util.Scanner;

public class ABC {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter First no");
		
		int a=sc.nextInt();
		
		System.out.println("Enter Second no");
		
		int b=sc.nextInt();
		System.out.println(a+"  "+b);

	}

}
