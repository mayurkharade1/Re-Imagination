package testCases;

import org.testng.annotations.Test;

import pageObjects.ChequeBookIssue;
import pageObjects.HomePage;
import testBase.BaseClass;

public class TC02_ChequeBookIssuance extends BaseClass{

	@Test
	public void Cheque_Book_Issue() {
		
		logger.info("** ChequeBook Issuance Started ***");
		
		HomePage hp= new HomePage(driver);
		
		hp.clickOnChequeBook();
		logger.info("** ChequeBook Issuance Started ***");
		
		ChequeBookIssue ch= new ChequeBookIssue(driver);
		
		ch.EnterAccNo("50003413184");
		logger.info("** Account No Entered***");
		ch.clickOnProceed();
		logger.info("** Clicked on proceed after account no input ***");
		ch.clickOnProceedtoConfirm();
		logger.info("** Clicked on proceed to navigate confirmation page ***");
		ch.clickOnProceedonConfirmationPage();
		logger.info("** Clicked on proceed from confirmation page ***");
		ch.EnterAgentPassword("Def@12345");
		logger.info("** ChequeBook Issuance Started ***");
		ch.clickOnconsentYesBtn();
		logger.info("** ChequeBook Issuance Started ***");
		ch.clickOnFpScannerCustAuth();
		logger.info("** ChequeBook Issuance Started ***");
		ch.clickOnproceedCustAuth();
		logger.info("** ChequeBook Issuance Started ***");
		
		
		
	}
	
}
