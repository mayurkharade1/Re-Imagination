package pageObjects;

import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;
import org.openqa.selenium.support.FindBy;

public class ChequeBookIssue extends BasePage{

	public ChequeBookIssue(WebDriver driver) {
		super(driver);
	}
	
	@FindBy(xpath = "//input[@placeholder='Enter Account number']")
	WebElement txtAccNo;
	@FindBy(xpath = "//p[text()='Proceed']")
	WebElement btnProceed;
	@FindBy(xpath = "//p[text()='proceed']")
	WebElement btnProceedCheqHome;
	@FindBy(xpath = "//p[text()='PROCEED']")
	WebElement btnProceedConfiPage;
	@FindBy(xpath = "//input[@type='password']")
	WebElement txtAgentAuth;
	@FindBy(xpath = "/html/body/div[3]/div/div/div/div[2]/div/div[2]/div/button/p")
	WebElement btnProceedAgentauth;
	@FindBy(xpath = "//button[text()='YES, I DO']")
	WebElement btnConsentYes;
	
	
	public void EnterAccNo(String AccNo) {
		txtAccNo.sendKeys(AccNo);
	}
	
	public void clickOnProceed() {
		btnProceed.click();
	}
	
	public void clickOnProceedtoConfirm() {
		btnProceedCheqHome.click();
	}
	
	public void clickOnProceedonConfirmationPage() {
		btnProceedConfiPage.click();
	}
	
	public void EnterAgentPassword(String agentPass) {
		txtAgentAuth.sendKeys(agentPass);
		btnProceedAgentauth.click();
	}
	
	public void clickOnconsentYesBtn() {
		btnProceedConfiPage.click();
	}
}
